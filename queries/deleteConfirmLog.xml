<query id="deleteConfirmLog" action="delete">
    <tables>
        <table name="gradeup_confirm_group" />
    </tables>
    <conditions>
		<condition operation="equal" column="log_srl" var="log_srl" filter="number" />
        <condition operation="equal" column="member_srl" var="member_srl" />
        <condition operation="equal" column="state" var="state" pipe="and" />
    </conditions>
</query>
